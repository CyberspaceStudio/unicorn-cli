{
  "name": "unicorn-cli",
  "version": "1.0.7",
  "description": "unicorn-cli",
  "main": "index.js",
  "scripts": {
    "compile": "babel src -d dist",
    "watch": "npm run compile -- --watch",
    "lint:fix": "eslint --fix --ext .js src",
    "commit": "git add . && git-cz"
  },
  "bin": {
    "unicorn-cli": "./bin/www"
  },
  "keywords": [
    "cli",
    "fe"
  ],
  "author": "yining_byte",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.8.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.3.0"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "chalk": "^4.1.0",
    "commander": "^6.1.0",
    "download-git-repo": "^3.0.2",
    "inquirer": "^7.3.3",
    "lint-staged": "^10.3.0",
    "mkdirp": "^1.0.4",
    "ora": "^5.0.0",
    "promisfy": "^1.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run compile"
    }
  },
  "lint-staged": {
    "src/*_/_.{js}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
